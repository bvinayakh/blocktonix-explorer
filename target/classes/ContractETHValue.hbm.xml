<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ContractETHValue" table="contract_eth_value">
        <meta attribute="class-description">
            Contract ETH Value
        </meta>
        <id name="id" type="string" column="contract_txn">
            <generator class="native" />
        </id>
        <property name="contract_symbol" column="contract_symbol" type="string" />
        <property name="contract_address" column="contract_address" type="string" />
        <property name="contract_txn" column="contract_txn" type="string" />
        <property name="contract_amount" column="contract_amount" type="string" />
        <property name="contract_value_eth" column="contract_value_eth" type="string" />
        <property name="contract_block_number" column="contract_block_number" type="string" />
        <property name="contract_block_time" column="contract_block_time" type="date" />
        <property name="contract_value_usd" column="contract_value_usd" type="string" />
    </class>
</hibernate-mapping>
    
