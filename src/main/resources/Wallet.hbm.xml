<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="Wallet" table="wallet_information">
        <meta attribute="class-description">
            Wallet Details
        </meta>
        <id name="id" type="string" column="wallet_balance_at">
            <generator class="native" />
        </id>
        <property name="wallet_address" column="wallet_address" type="string" />
        <property name="wallet_eth_balance" column="wallet_eth_balance" type="string" />
        <property name="wallet_balance_at" column="wallet_balance_at" type="string" />
    </class>
</hibernate-mapping>
    
